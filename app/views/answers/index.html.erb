<h1> Archived Questions and Answers for <%=params[:subject]%> </h1>
		<% if @questions %>
			<% @questions.each do |question| %>
			  <div class="panel panel-warning">
          <div class="panel-heading">
						<h3 class="panel-title">question.title</h3>
					</div>

					<div class="panel-body"> Question asked: <%= question.content %></div>

					<div class="panel-body"> Your response: <%= question.answer.content %></div>
			<% end %>
		</div>
		<% else %>
			<%= "No questions available" %>
			<%= link_to "Go Back", root_path %>
		<% end %>
	</div>